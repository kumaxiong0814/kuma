WITH Original AS
(
  SELECT
    '0174' AS InterfaceId,
    '0' AS ErrorClassification,
    IIF(
      ISNULL(OrderSource, '') = '',
      OrderSource,
      CONCAT(SUBSTRING(OrderSource, 1, 2), SUBSTRING(CONVERT(VARCHAR(10), IF0174Seq), 3, 8))
    ) AS OhiId,
    '0' AS Status,
    IIF(
      ISNULL(OrderSource, '') = '',
      OrderSource,
      CONCAT(SUBSTRING(OrderSource, 1, 3), IF0174Seq)
    ) AS OrigSysDocumentRef,
    CONVERT(INT, NULLIF(OrderSource, '')) AS OrderSource,
    CustomerPoNumber,
    CustomerNumber,
    ShipmentPriorityCode,
    ShipToOrgCode,
    OrganizationCode,
    SecondaryInventoryName,
    IIF(
      ISNULL(OrderSource, '') = '',
      OrderSource,
      CONCAT(SUBSTRING(OrderSource, 1, 2), SUBSTRING(CONVERT(VARCHAR(10), IF0174Seq), 3, 8))
    ) AS OliId,
    OrigSysLineRef,
    InventoryItemSegment1,
    OrderedQuantity,
    InvoiceToOrgCode,
    '' As ErrorMessage,
    '' AS SalesTableOIFRecId,
    DENSE_RANK() OVER (ORDER BY OrderSource, CustomerPoNumber) AS GroupNum
  FROM earth_dp_SpoonsOrderIF_LS_IF_0174_TempWithSeq
),
GroupHeader AS
(
  SELECT
    ROW_NUMBER() OVER (ORDER BY OliId) - 1 AS GroupNum,
    OhiId
  FROM Original
  WHERE OhiId <> ''
),
DataSource AS
(
  SELECT
    InterfaceId,
    ErrorClassification,
    COALESCE(GroupHeader.OhiId, '') AS OhiId,
    Status,
    OrigSysDocumentRef,
    OrderSource,
    CustomerPoNumber,
    CustomerNumber,
    ShipmentPriorityCode,
    ShipToOrgCode,
    OrganizationCode,
    OliId,
    SecondaryInventoryName,
    OrigSysLineRef,
    InventoryItemSegment1,
    OrderedQuantity,
    InvoiceToOrgCode,
    ErrorMessage,
    SalesTableOIFRecId
  FROM Original
  LEFT JOIN GroupHeader ON Original.GroupNum = GroupHeader.GroupNum
)
SELECT
  CustomerNumber,
  CustomerPoNumber,
  ErrorClassification,
  ErrorMessage,
  InterfaceId,
  InventoryItemSegment1,
  InvoiceToOrgCode,
  OhiId,
  OliId,
  OrderSource,
  OrderedQuantity,
  OrganizationCode,
  OrigSysDocumentRef,
  OrigSysLineRef,
  SecondaryInventoryName,
  ShipToOrgCode,
  ShipmentPriorityCode,
  SalesTableOIFRecId,
  Status
FROM DataSource;
